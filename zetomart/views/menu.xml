<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- This Menu Item will appear in the Upper bar, that's why It needs NO parent or action -->
    <menuitem id="zetomart_menu_root" 
              name="Zeto Mart" 
              sequence="10"/>

    <!-- This Menu Item Must have a parent -->
    <menuitem id="zetomart_barang_menu_categ" 
              name="Barang" 
              parent="zetomart_menu_root" 
              sequence="10"/>

    <!-- This Menu Item Must have a parent -->
    <menuitem id="zetomart_pegawai_menu_categ" 
              name="Pegawai" 
              parent="zetomart_menu_root" 
              sequence="20"/>



    <!-- zetomart_transakasi Item Must have a parent -->
    <menuitem id="zetomart_transakasi_menu_categ" 
              name="Transaksi" 
              parent="zetomart_menu_root" 
              sequence="42"/>
</odoo>
